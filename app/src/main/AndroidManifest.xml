<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codembeded.jutttravelsco">

    <application
        android:name=".helperclass.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.ForgetPassword"
            android:label="Forget Password"></activity>
        <activity
            android:name=".activity.OurTours"
            android:label="Our Tours" />
        <activity
            android:name=".activity.MyHistory"
            android:label="My History" />
        <activity
            android:name=".activity.BookMyTicket"
            android:label="Book My Ticket" />
        <activity
            android:name=".activity.SpecialBooking"
            android:label="Special Booking" />
        <activity
            android:name=".activity.BookTour"
            android:label="Book Tour" />
        <activity
            android:name=".activity.Complaints"
            android:label="Complaints" />
        <activity
            android:name=".activity.MyBookings"
            android:label="My Bookings" />
        <activity
            android:name=".activity.SignUp"
            android:label="Sign Up" />
        <activity
            android:name=".activity.Home"
            android:label="Home" />
        <activity
            android:name=".activity.Login"
            android:label="Jutt Travels" />
        <activity android:name=".activity.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>